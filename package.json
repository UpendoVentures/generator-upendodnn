  },
  "scripts": {
    "test": "jest",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true

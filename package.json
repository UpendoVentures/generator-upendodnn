  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UpendoVentures/generator-upendodnn/issues"
  },
  "packageConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "resolutions": {
    "**/**/got": "11.8.5",
    "**/**/semver-regex": "3.1.4",
    "**/**/yargs-parser": "13.1.2",
    "**/**/mem": "4.0.0",
    "**/**/lodash": "4.17.21",
    "**/**/trim-newlines": "3.0.1",
    "**/**/http-cache-semantics": "4.1.1"
  }
